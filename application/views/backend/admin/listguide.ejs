    <script>
    function shoDetailTypeCard() {
        var NewType = $('#NewType').val();
        var url = "/mainadmin/getDeailTypeCard";
        postData = {
            'NewType' : NewType
        };
        $.ajax({
            type: "POST",
            dataType: "json",
            cache: false,
            url: url,
            data: postData,
            success: function(json) {
                var listDetailTypeCard = json;
                var maxLoop = listDetailTypeCard.length;
                $('#NewDetailType').empty();
                if ( !$('#NewDetailType').val() ) {
                    $('#NewDetailType').append('<option value="">-- เลือกชนิดบัตร --</option>');
                }
                for (var i=0 ; i < maxLoop; i++) {
                    var detailType = listDetailTypeCard[i]['detailType'];
                    var codeTypeDetail = listDetailTypeCard[i]['codeTypeDetail'];
                        $('#NewDetailType').append('<option value="'+codeTypeDetail+'">'+detailType+'</option>');
                }
            },
            error: function() {
                console.log('package load failed...');
            }
        });
    }
    function chkNewdata() {
        var Newuser = $("#Newuser").val();
        var Newpass = $("#Newpass").val();
        var NewFname = $("#NewFname").val();
        var NewLname = $("#NewLname").val();
        var NewFnameEN = $("#NewFnameEN").val();
        var NewLnameEN = $("#NewLnameEN").val();
        var NewIDCard = $("#NewIDCard").val();
        var NewPassport = $("#NewPassport").val();
        var NewTel = $("#NewTel").val();
        var NewEmail = $("#NewEmail").val();
        var NewType = $("#NewType").val();
        var NewDetailType = $("#NewDetailType").val();
        var NewImgCard = $("#NewImgCard").val();
        var NewNoCard = $("#NewNoCard").val();
        var endGuide = $("#endGuide").val();
        var html = "" ;
        var chkInput = 1;
        var textShow = "";
        
            if ( Newuser == "" && Newpass == "" ) { chkInput = 0; var textShow = 'กรุณากรอก ข้อมูลให้ครบถ้วน'; }
            else if ( Newuser == "" ) { chkInput = 0; var textShow = 'กรุณากรอก Username'; }
