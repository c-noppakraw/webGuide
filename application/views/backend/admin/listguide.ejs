                                            var fiestname = dataList.fiestname ;
                                            var lastname = dataList.lastname ;
                                            var phone = dataList.phone ;
                                            var email = dataList.email ;
                                            var address = dataList.address;
                                            var district = dataList.district;
                                            var amphoe = dataList.amphoe;
                                            var province = dataList.province;
                                            var zipcode = dataList.zipcode;
                                            var cardVisible = dataList.cardVisible;
                                            var detailCard = dataList.detailCard;
                                            var detailType = dataList.detailType;
                                            var noGuide = dataList.noGuide;
                                            var adminUpdate = dataList.adminUpdate ;
                                            var dateUpdate = dataList.dateUpdate ;
                                    %>
                                        <tr>
                                            <% if(status == 1) { %>
                                                <td class="text-center" style="background-color: #76D7C4 ; color: #f9f9f9 ; "><p style="margin-top: 10px; margin-bottom: 0px;">เปิด</p></td>
